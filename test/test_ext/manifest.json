{
    "name": "Test extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "128": "./img/icon_128.png"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "browser_action": {
        "default_icon": "./img/icon_128.png",
        "default_popup": "main.html"
    },
    "permissions": [
        "http://google.com",
        "tabs"
    ]
}